    public class Settings
    {
        string wsCommerciaNet;
        public string WsCommerciaNet
        {
            get
            {
                return App.settingsContainer.Containers["CommerciaCATTLE"].Values["WsCommerciaNet"].ToString();
            }

            set
            {
                wsCommerciaNet = value;
                App.settingsContainer.Containers["CommerciaCATTLE"].Values["WsCommerciaNet"] = value;
            }
        }

        string wsMobile;
        public string WsMobile
